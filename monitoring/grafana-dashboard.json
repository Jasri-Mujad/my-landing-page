{
    "annotations": {
        "list": []
    },
    "title": "Jasri API Dashboard",
    "uid": "jasri-api-dashboard",
    "panels": [
        {
            "id": 1,
            "title": "HTTP Requests Total",
            "type": "stat",
            "gridPos": {
                "h": 4,
                "w": 6,
                "x": 0,
                "y": 0
            },
            "targets": [
                {
                    "expr": "sum(jasri_api_http_requests_total)",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "short",
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            }
                        ]
                    }
                }
            }
        },
        {
            "id": 2,
            "title": "Memory Usage (Heap)",
            "type": "gauge",
            "gridPos": {
                "h": 4,
                "w": 6,
                "x": 6,
                "y": 0
            },
            "targets": [
                {
                    "expr": "jasri_api_nodejs_heap_size_used_bytes / 1024 / 1024",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "decmbytes",
                    "min": 0,
                    "max": 512,
                    "thresholds": {
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 256
                            },
                            {
                                "color": "red",
                                "value": 400
                            }
                        ]
                    }
                }
            }
        },
        {
            "id": 3,
            "title": "Active Handles",
            "type": "stat",
            "gridPos": {
                "h": 4,
                "w": 6,
                "x": 12,
                "y": 0
            },
            "targets": [
                {
                    "expr": "jasri_api_nodejs_active_handles_total",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "short",
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            }
                        ]
                    }
                }
            }
        },
        {
            "id": 4,
            "title": "API Status",
            "type": "stat",
            "gridPos": {
                "h": 4,
                "w": 6,
                "x": 18,
                "y": 0
            },
            "targets": [
                {
                    "expr": "up{job=\"jasri-api\"}",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "mappings": [
                        {
                            "type": "value",
                            "options": {
                                "1": {
                                    "text": "UP",
                                    "color": "green"
                                }
                            }
                        },
                        {
                            "type": "value",
                            "options": {
                                "0": {
                                    "text": "DOWN",
                                    "color": "red"
                                }
                            }
                        }
                    ]
                }
            }
        },
        {
            "id": 5,
            "title": "HTTP Requests Over Time",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 4
            },
            "targets": [
                {
                    "expr": "rate(jasri_api_http_requests_total[5m])",
                    "legendFormat": "{{method}} {{route}}",
                    "refId": "A"
                }
            ]
        },
        {
            "id": 6,
            "title": "Memory Usage Over Time",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 4
            },
            "targets": [
                {
                    "expr": "jasri_api_nodejs_heap_size_used_bytes / 1024 / 1024",
                    "legendFormat": "Heap Used (MB)",
                    "refId": "A"
                },
                {
                    "expr": "jasri_api_nodejs_heap_size_total_bytes / 1024 / 1024",
                    "legendFormat": "Heap Total (MB)",
                    "refId": "B"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "decmbytes"
                }
            }
        },
        {
            "id": 7,
            "title": "Requests by Status Code",
            "type": "piechart",
            "gridPos": {
                "h": 8,
                "w": 8,
                "x": 0,
                "y": 12
            },
            "targets": [
                {
                    "expr": "sum by (status) (jasri_api_http_requests_total)",
                    "legendFormat": "{{status}}",
                    "refId": "A"
                }
            ]
        },
        {
            "id": 8,
            "title": "Event Loop Lag",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 8,
                "x": 8,
                "y": 12
            },
            "targets": [
                {
                    "expr": "jasri_api_nodejs_eventloop_lag_seconds",
                    "legendFormat": "Event Loop Lag",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s"
                }
            }
        },
        {
            "id": 9,
            "title": "Process CPU Usage",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 8,
                "x": 16,
                "y": 12
            },
            "targets": [
                {
                    "expr": "rate(jasri_api_process_cpu_seconds_total[5m]) * 100",
                    "legendFormat": "CPU %",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percent"
                }
            }
        }
    ],
    "schemaVersion": 38,
    "timezone": "browser",
    "refresh": "30s"
}